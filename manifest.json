{
	"name": "OSSNATOR",
	"description": "A browser extension for internal use which opens an Incognito window with a specific URL to impersonate a User's permissions.",
	"version": "0.1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"storage",
		"tabs"
	],
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "/images/icon-128.png",
			"32": "/images/icon-128.png"
		}
	},
	"options_page": "options.html",
	"icons": {
		"16": "/images/icon-128.png",
		"32": "/images/icon-128.png",
		"48": "/images/icon-128.png",
		"128": "/images/icon-128.png"
	}
}
